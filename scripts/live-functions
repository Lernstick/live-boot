
. /scripts/functions

# Override this so we don't call PROGRESS
log_end_msg()
{
	if [ -x /sbin/usplash_write ]; then
		/sbin/usplash_write "SUCCESS ok"
	fi
	_log_msg "Done."
}

# Print a message and wait for enter
log_wait_msg()
{
	if [ -x /sbin/usplash_write ]; then
		/sbin/usplash_write "INPUTENTER $@"
		read nunya < /dev/.initramfs/usplash_outfifo
	fi
	_log_msg "Waiting: $@ ..."
}
